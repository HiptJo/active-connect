{
  "name": "active-connect",
  "version": "2.7.0",
  "description": "Connection framework built for smart web-based projects using NodeJS, Angular and Websockets.",
  "author": "Jonas Hiptmair <office@hiptmairit.at>",
  "keywords": [
    "API",
    "WS",
    "websocket",
    "node server"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.ts -t verbose",
    "test": "jest --watch --passWithNoTests --detectOpenHandles --forceExit",
    "test:prod": "jest --passWithNoTests --detectOpenHandles --forceExit",
    "coverage": "jest --passWithNoTests --detectOpenHandles --forceExit --coverage",
    "tsc": "tsc",
    "prepublish": "npm run build",
    "docs": "typedoc"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^9.0.7",
    "@types/geoip-lite": "^1.4.1",
    "@types/lodash": "^4.14.195",
    "@types/randomstring": "^1.1.8",
    "@types/ws": "8.5.4",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "fs-extra": "^11.1.1",
    "geoip-lite": "^1.4.7",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.2",
    "randomstring": "^1.2.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^5.1.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.24.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiptJo/active-connect.git"
  },
  "bugs": {
    "url": "https://github.com/HiptJo/active-connect/issues"
  },
  "directories": {
    "lib": "dist",
    "doc": "doc"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true
}
