{
  "name": "active-connect",
  "version": "1.11.1",
  "description": "Connection framework built for smart web-based projects using NodeJS, Angular and Websockets.",
  "author": "Jonas Hiptmair <office@hiptmairit.at>",
  "keywords": [
    "API",
    "WS",
    "websocket",
    "node server"
  ],
  "main": "./dist/active-connect.js",
  "typings": "./dist/active-connect.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.ts -t verbose",
    "test": "jest --watch --passWithNoTests --detectOpenHandles --forceExit",
    "test:prod": "jest --passWithNoTests --detectOpenHandles --forceExit",
    "coverage": "jest --passWithNoTests --detectOpenHandles --forceExit --coverage",
    "tsc": "tsc",
    "prepublish": "npm run build",
    "docs": "typedoc"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@types/geoip-lite": "^1.4.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fs-extra": "9.0.1",
    "geoip-lite": "^1.4.2",
    "jest": "^26.6.3",
    "node-cron": "^3.0.2",
    "randomstring": "^1.2.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^4.1.3",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "^26.0.20",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7.4.0",
    "typedoc": "^0.24.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiptJo/active-connect.git"
  },
  "bugs": {
    "url": "https://github.com/HiptJo/active-connect/issues"
  },
  "directories": {
    "lib": "dist",
    "doc": "doc"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true
}
